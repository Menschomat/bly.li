{
  "name": "front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "$npm_execpath run ng serve",
    "start:container": "$npm_execpath run start --host 0.0.0.0 --poll 2000 --disable-host-check",
    "start:container:blyli": "$npm_execpath run start:container --proxy-config proxy/blyli.proxy.conf.json",
    "start:container:local": "$npm_execpath run start:container --proxy-config proxy/local.proxy.conf.json",
    "build": "$npm_execpath run ng build",
    "watch": "$npm_execpath run ng build --watch --configuration development",
    "test": "$npm_execpath run ng test",
    "generate:api": "openapi-generator-cli generate -i ../shortn/api/openapi.yml -g typescript-angular -o src/app/core/api/v1 --additional-properties fileNaming=kebab-case,withInterfaces=true --generate-alias-as-model"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.3",
    "@angular/common": "^19.1.3",
    "@angular/compiler": "^19.1.3",
    "@angular/core": "^19.1.3",
    "@angular/forms": "^19.1.3",
    "@angular/platform-browser": "^19.1.3",
    "@angular/platform-browser-dynamic": "^19.1.3",
    "@angular/router": "^19.1.3",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "angular-oauth2-oidc": "^17.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.4",
    "@angular/cli": "^19.1.4",
    "@angular/compiler-cli": "^19.1.3",
    "@openapitools/openapi-generator-cli": "^2.13.5",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.20",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.5.2"
  }
}
