{
  "name": "front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --poll 2000 --disable-host-check --proxy-config proxy/devcontainer.proxy.conf.json",
    "start:blyli": "ng serve --host 0.0.0.0 --poll 2000 --disable-host-check --proxy-config proxy/blyli.proxy.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate:api": "openapi-merge-cli && openapi-generator-cli generate   -i ../../api/frontend.openapi.json  -g typescript-angular -o src/app/api/ --ignore-file-override ./.openapi-generator-ignore --additional-properties fileNaming=kebab-case,withInterfaces=true,ngVersion=19, --generate-alias-as-model"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.3",
    "@angular/common": "^19.1.3",
    "@angular/compiler": "^19.1.3",
    "@angular/core": "^19.1.3",
    "@angular/forms": "^19.1.3",
    "@angular/platform-browser": "^19.1.3",
    "@angular/platform-browser-dynamic": "^19.1.3",
    "@angular/router": "^19.1.3",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@tailwindcss/postcss": "^4.0.0",
    "angular-oauth2-oidc": "^19.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.4",
    "@angular/cli": "^19.1.4",
    "@angular/compiler-cli": "^19.1.3",
    "@openapitools/openapi-generator-cli": "^2.16.2",
    "@types/jasmine": "~5.1.5",
    "autoprefixer": "^10.4.20",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.7.3"
  }
}
